{
    "version": "0.2.0",
   "configurations": [
   {
   "name": ".NET Core Launch (web)",
   "type": "coreclr",
   "request": "launch",
   "preLaunchTask": "build",
   "program": "${workspaceFolder}/bin/Debug/net8.0/SysCribBackend.dll",
   "args": [],
   "cwd": "${workspaceFolder}",
   "stopAtEntry": false,
   "env": {
   "ASPNETCORE_ENVIRONMENT": "Development"
   },
   "sourceFileMap": {
   "/Views": "${workspaceFolder}/Views"
   }
   },
   {
   "name": ".NET Core Attach",
   "type": "coreclr",
   "request": "attach"
   }
   ]
   }